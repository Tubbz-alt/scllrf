file GUNBWavePerChassis.template
{
pattern{ P,				CHASSIS,	KEEP,		PORT }
	{ "GUN:GUNB:100:",	"PRC:",	"0xC3C3",	PRCReg }
	{ "GUN:GUNB:100:",	"RFS1:",	"0xFFFF",	RFS1Reg }
	{ "GUN:GUNB:100:",	"RFS2:",	"0xFFFF",	RFS2Reg }
}

