record(bo, "$(P)I_Q_BIT_WIDTH")
{
    field(DESC, "I/Q waveforms 22 bit/16k or 16 bit/32k")
    field(PINI, "NO")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),0) I_Q_BIT_WIDTH")
    field(SCAN, "Passive")
    field(ZNAM, "22 bit")
    field(ONAM, "16 bit")
    info(autosaveFields, "VAL")
}

