	{ "ACCL:GUNB:455:RFS1:SPARE3:",	"8",	"9",	"RFS1Reg",	"sqrt(W)"	}
	{ "ACCL:GUNB:455:RFS1:DAC:",	"9",	"10",	"RFS1Reg",	"sqrt(W)"	}
