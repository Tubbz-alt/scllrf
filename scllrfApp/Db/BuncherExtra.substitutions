file GUNBWaveCh.template {
pattern { P,					 CH,	 CH+1,	 PORT,		 EGU	}
	{ "ACCL:GUNB:455:PRC:SPARE1:",	"0",	"1",	"PRCReg",	""	}
	{ "ACCL:GUNB:455:PRC:SPARE2:",	"1",	"2",	"PRCReg",	""	}
	{ "ACCL:GUNB:455:PRLFWD:",	"2",	"3",	"PRCReg",	"dBm"	}
	{ "ACCL:GUNB:455:PRLREV:",	"3",	"4",	"PRCReg",	"dBm"	}
